{
  "version": 2,
  "buildCommand": "cd admin-panel; npm run build",
  "installCommand": "cd \"auto blog-appv1\"; npm install --legacy-peer-deps; cd ../admin-panel; npm install --legacy-peer-deps",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin-panel/build/:path*"
    },
    {
      "source": "/admin",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/login",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/articles/:path*",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/analytics",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/users",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/categories",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/comments",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/media",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/settings",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/activity",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/authors",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/newsletter",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/search",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/tags",
      "destination": "/admin-panel/build/index.html"
    },
    {
      "source": "/css/:path*",
      "destination": "/auto blog-appv1/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/auto blog-appv1/js/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/auto blog-appv1/images/:path*"
    },
    {
      "source": "/fonts/:path*",
      "destination": "/auto blog-appv1/fonts/:path*"
    },
    {
      "source": "/article.html",
      "destination": "/auto blog-appv1/article.html"
    },
    {
      "source": "/author.html",
      "destination": "/auto blog-appv1/author.html"
    },
    {
      "source": "/index.html",
      "destination": "/auto blog-appv1/index.html"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/auto blog-appv1/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(?!api|admin|login|dashboard|articles|analytics|users|categories|comments|media|settings|activity|authors|newsletter|search|tags|css|js|images|fonts|article\\.html|author\\.html|index\\.html).*",
      "dest": "/auto blog-appv1/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
